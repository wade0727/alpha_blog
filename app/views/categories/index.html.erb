<div class="well">
  <div class="page-header">
    <h1 align="center">Listing All Categories</h1>
  </div>
  <div align="center">
    <% @categories.each do |category| %>
      <ul class="listing">
        <div class="row">
          <div class="col-md-4 col-md-offset-4">
            <li class="list-group">
              <strong><%= link_to "#{category.name}", category_path(category), class: "list-group-item list-group-item-success" %></strong>
            </li>
          </div>
        </div>
      </ul>
    <% end %>
  </div>
</div>